<p style="color: green"><%= notice %></p>

<% content_for :title, "Clients" %>

<h1>Clients</h1>

<div id="clients">
  <% @clients.each do |client| %>
    <%= render client %>
    <p>
      <%= link_to "Show this client", client %>
    </p>
  <% end %>
</div>

<div class="nav-links">
  <%= link_to "New client", new_client_path, class: "button" %>
  <%= link_to "Manage Strengths", strengths_path, class: "button" %>
</div>
